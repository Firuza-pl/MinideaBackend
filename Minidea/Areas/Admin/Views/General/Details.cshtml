@{
    @model StaticData
    ViewData["Title"] = "Details";
}

<style>

</style>

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">İdarə tablosuna bax</h4>
                <hr>
                <section class="wrapper" style="padding-bottom:50px;">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="bot-links">
                                <p class="mt-3">
                                    <button type="button" class="btn btn-social-icon btn-outline-sales">
                                        <i class="mdi mdi-cellphone"></i>
                                    </button>
                                    &nbsp; @Model.PhoneOne
                                </p>
                                <hr />
                                <p class="mt-3">
                                    <button type="button" class="btn btn-social-icon btn-outline-sales">
                                        <i class="mdi mdi-cellphone"></i>
                                    </button> &nbsp; @Model.MobileTwo
                                </p>
                                <hr />
                                <p class="mt-3">
                                    <button type="button" class="btn btn-social-icon btn-outline-sales">
                                        <i class="mdi mdi-cellphone"></i>
                                    </button> &nbsp; @Model.MobileThree
                                </p>
                                <hr />
                                <p class="mt-3">
                                    <button type="button" class="btn btn-social-icon btn-outline-sales">
                                        <i class="mdi mdi-email"></i>
                                    </button> &nbsp;<a href="mailto:@Model.Email" target="_blank"> @Model.Email </a>
                                </p>
                                <hr />
                                <p class="mt-3">
                                    <button type="button" class="btn btn-social-icon btn-outline-sales">
                                        <i class="mdi mdi-link"></i>
                                    </button> &nbsp;<a href="@Model.FacebookLink" target="_blank"> @Model.FacebookLink </a>
                                </p>
                                <hr />
                                <p class="mt-3">
                                    <button type="button" class="btn btn-social-icon btn-outline-sales">
                                        <i class="mdi mdi-link"></i>
                                    </button> &nbsp; <a href="@Model.InstagramLink" target="_blank"> @Model.InstagramLink</a>
                                </p>
                                <hr />
                                <p class="mt-3">
                                    <button type="button" class="btn btn-social-icon btn-outline-sales">
                                        <i class="mdi mdi-link"></i>
                                    </button> &nbsp;  <a href="@Model.LinkedinLink" target="_blank"> @Model.LinkedinLink </a>
                                </p>
                                <hr />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-12 text-center">
                            <a class="btn btn-dark" asp-action="Generals">Əsas Səhifəyə qayıt</a>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</div>

